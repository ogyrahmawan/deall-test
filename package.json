{
  "name": "deall",
  "version": "1.0.0",
  "description": "API for CRUD user",
  "main": "index.js",
  "scripts": {
    "build": "npm run tsc && babel src -d dist --extensions \".ts,.js\" --copy-files",
    "start": "node dist/server.js",
    "tsc": "tsc --noEmit",
    "local": "export NODE_ENV=localhost && nodemon -L --exec babel-node src/server.ts --extensions \".ts\"",
    "stag": "export NODE_ENV=staging && nodemon -L --exec babel-node src/server.ts --extensions \".ts\"",
    "prod": "export NODE_ENV=preprod && npm run start",
    "lint": "eslint . --fix --ext .ts,.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "babel-node": "^0.0.1-security",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^4.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.189",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^8.27.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^3.1.5",
    "nodemon": "^2.0.20",
    "typescript": "^4.5.3"
  }
}
